	<!-- GOC Security Armor -->
	<ThingDef ParentName="ArmorMachineableBase">
		<defName>SCP_GOCSecurityArmor</defName>
		<label>Security Armor (GOC)</label>
		<description>An Armored vest worn by GOC Security members.</description>
		<techLevel>Industrial</techLevel>
		<recipeMaker>
			<researchPrerequisite>FlakArmor</researchPrerequisite>
			<unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
			<skillRequirements>
				<Crafting>7</Crafting>
			</skillRequirements>
		</recipeMaker>
		<graphicData>
			<texPath>Things/Apparel/GOC/GOC_SecurityArmor/GOC_SecurityArmor</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>0.90</drawSize>
		</graphicData>
		<statBases>
			<WorkToMake>9000</WorkToMake>
			<MaxHitPoints>250</MaxHitPoints>
			<Mass>4.0</Mass>
			<Flammability>0.5</Flammability>
			<ArmorRating_Sharp>1.00</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.58</ArmorRating_Blunt>
			<ArmorRating_Heat>0.27</ArmorRating_Heat>
			<Insulation_Cold>1</Insulation_Cold>
			<EquipDelay>5.5</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>-0.15</MoveSpeed>
			<AimingDelayFactor>-0.15</AimingDelayFactor>
		</equippedStatOffsets>
		<thingCategories>
			<li>ApparelArmor</li>
		</thingCategories>
		<costList>
			<Cloth>20</Cloth>
			<Steel>75</Steel>
		</costList>
		<apparel>
			<careIfWornByCorpse>false</careIfWornByCorpse>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Neck</li>
				<li>Shoulders</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Apparel/GOC/GOC_SecurityArmor/GOC_SecurityArmor</wornGraphicPath>
			<layers>
				<li>Middle</li>
			</layers>
			<tags>
				<!--<li>IndustrialMilitaryBasic</li>-->
				<li>GOC_Security</li>
			</tags>
			<defaultOutfitTags>
				<li>Soldier</li>
			</defaultOutfitTags>
			<useWornGraphicMask>true</useWornGraphicMask>
		</apparel>
		<tradeTags>
			<li>Armor</li>
			<li>SPC_Armor</li>
		</tradeTags>
	</ThingDef>
	
