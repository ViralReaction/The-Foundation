<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- 2687 Recipe -->
	<RecipeDef>
		<defName>Foundation_Make_Plastic_Organ</defName>
		<label>make scp-2687-a model</label>
		<description>Make Plastic Organ Model.</description>
		<jobString>Making Plastic Organ Model.</jobString>
		<workSpeedStat>DrugCookingSpeed</workSpeedStat>
		<workSkill>Cooking</workSkill>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_Brewing</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<targetCountAdjustment>1</targetCountAdjustment>
		<recipeUsers>
			<li>DrugLab</li>
		</recipeUsers>
		<skillRequirements>
			<Crafting>5</Crafting>
		</skillRequirements>
		<workAmount>1000</workAmount>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Neutroamine</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Plasteel</li>
					</thingDefs>
				</filter>
				<count>15</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Neutroamine</li>
				<li>Plasteel</li>
			</thingDefs>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<thingDefs>
				<li>Neutroamine</li>
				<li>Plasteel</li>
			</thingDefs>
		</defaultIngredientFilter>
		<products>
			<Foundation_Plastic_Organ_Model>1</Foundation_Plastic_Organ_Model>
		</products>
	</RecipeDef>

