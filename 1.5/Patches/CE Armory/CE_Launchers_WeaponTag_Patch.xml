<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Combat Extended Launchers</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

				<!-- Launchers -->
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[
					defName="CE_Launchers_RPGTwentyEight" or defName="CE_Launchers_RPGThirtyTwo"]/weaponTags
					</xpath>
					<value>
						<li>Foundation_Combloc_Launcher</li>
					</value>
				</li>

				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[
					defName="CE_Launchers_MOneFourOne" or
					defName="CE_Launchers_SMAW"]/weaponTags
					</xpath>
					<value>
						<li>Foundation_US_Launcher</li>
					</value>
				</li>

				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="CE_Launchers_MATADOR"]/weaponTags</xpath>
					<value>
						<li>Foundation_GOC_Launcher</li>
					</value>
				</li>

			</operations>
		</match>
	</Operation>
</Patch>